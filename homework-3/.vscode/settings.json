{
  "editor.wordWrap": "on",
  "editor.tabSize": 4,
  "editor.fontFamily": "'Hack Nerd Font', 'XITS Math monospacified for Consolas', 'mononoki Nerd Font', 'Lucida Console', 'Noto Sans CJK TC Light', 'Consolas', 'Courier New', monospace",
  "latex-workshop.intellisense.package.enabled": true,
  "latex-workshop.latex.autoClean.run": "onBuilt",
  "latex-workshop.message.error.show": false,
  "latex-workshop.message.warning.show": false,
  "latex-workshop.view.pdf.viewer": "tab",
  "latex-workshop.view.pdf.external.viewer.command": "/usr/local/bin/displayline",
  "latex-workshop.view.pdf.external.viewer.args": ["0", "%PDF%"],
  "latex-workshop.view.pdf.external.synctex.command": "/usr/local/bin/displayline",
  "latex-workshop.view.pdf.external.synctex.args": [
    "-r",
    "%LINE%",
    "%PDF%",
    "%TEX%"
  ],
  "latex-workshop.latex.clean.fileTypes": [
    "*.aux",
    "*.bbl",
    "*.blg",
    "*.idx",
    "*.ind",
    "*.lof",
    "*.lot",
    "*.out",
    "*.toc",
    "*.acn",
    "*.acr",
    "*.alg",
    "*.glg",
    "*.glo",
    "*.gls",
    "*.ist",
    "*.fls",
    "*.log",
    "*.fdb_latexmk",
    "*.snm",
    "*.nav"
  ],
  "latex-workshop.latex.tools": [
    {
      "name": "latexmk",
      "command": "latexmk",
      "args": [
        "-synctex=1",
        "-interaction=nonstopmode",
        "-file-line-error",
        "-xelatex",
        "-shell-escape",
        "-outdir=%OUTDIR%",
        "%DOCFILE%"
      ]
    },

    {
      "name": "xelatex (debug)",
      "command": "xelatex",
      "args": [
        "-synctex=1",
        "-interaction=nonstopmode",
        "-shell-escape",
        "-file-line-error",
        "%DOCFILE%"
      ]
    },
    {
      "name": "xelatex",
      "command": "xelatex",
      "args": [
        "-synctex=1",
        "-interaction=batchmode",
        "-shell-escape",
        "-file-line-error",
        "%DOCFILE%"
      ]
    },
    {
      "name": "pdflatex (debug)",
      "command": "pdflatex",
      "args": [
        "-synctex=1",
        "-interaction=nonstopmode",
        "-shell-escape",
        "-file-line-error",
        "%DOC%"
      ]
    },
    {
      "name": "pdflatex",
      "command": "pdflatex",
      "args": [
        "-synctex=1",
        "-interaction=batchmode",
        "-shell-escape",
        "-file-line-error",
        "%DOC%"
      ]
    },
    {
      "name": "bibtex",
      "command": "bibtex",
      "args": ["%DOCFILE%"]
    }
  ],
  "latex-workshop.latex.recipes": [
    {
      "name": "xelatex (debug)",
      "tools": ["xelatex (debug)"]
    },
    {
      "name": "xelatex ➞ xelatex",
      "tools": ["xelatex", "xelatex"]
    },
    {
      "name": "xelatex",
      "tools": ["xelatex"]
    },
    {
      "name": "latexmk",
      "tools": ["latexmk"]
    },
    {
      "name": "pdflatex (debug)",
      "tools": ["pdflatex (debug)"]
    },
    {
      "name": "pdflatex",
      "tools": ["pdflatex"]
    }
  ],
  // "latex-workshop.view.pdf.invertMode.hueRotate": 360,
  // "latex-workshop.view.pdf.invertMode.sepia": 1,
  "latex-workshop.view.pdf.invert": 0.86,
  "latex-workshop.view.pdf.invertMode.brightness": 1.5,
  "latex-workshop.view.pdf.invertMode.grayscale": 0.5,
  "latex-workshop.latex.recipe.default": "lastUsed",
  "files.autoSave": "afterDelay",
  "files.autoSaveDelay": 3000,
  "latex-workshop.latex.verbatimEnvs": [
    "verbatim",
    "lstlisting",
    "minted",
    "markdown"
  ],
  // "latex-workshop.synctex.afterBuild.enabled": true,
  "latex-workshop.texcount.autorun": "onSave",
  "pasteImage.basePath": "${currentFileDir}",
  "pasteImage.defaultName": "YMMDD-HHmmss",
  "pasteImage.insertPattern": "![](${imageSyntaxPrefix}${imageFilePath}${imageSyntaxSuffix})",
  "pasteImage.path": "${currentFileDir}/assets/",
  "latex-workshop.bibtex-format.tab": "4 spaces",
  "latex-workshop.bind.enter.key": false,
  // "conceal.adjustCursorMovement": true,
  "conceal.revealOn": "active-line",
  "conceal.substitutions": [
    {
      "language": ["latex", { "pattern": "**/*.{tex}" }],
      "substitutions": [
        { "ugly": "\\\\forall", "pretty": "∀" },
        { "ugly": "\\\\exists", "pretty": "∃" },
        { "ugly": "\\\\nexists", "pretty": "∄" },
        { "ugly": "\\\\nexi", "pretty": "∄" },
        { "ugly": "\\\\Rightarrow", "pretty": "⇒" },
        { "ugly": "\\\\leq", "pretty": "≤" },
        { "ugly": "\\\\geq", "pretty": "≥" },
        // { "ugly": "\\\\in", "pretty": "∈" },
        { "ugly": "\\\\subset", "pretty": "⊂" },
        { "ugly": "\\\\subseteq", "pretty": "⊆" },
        { "ugly": "\\\\supset", "pretty": "⊃" },
        { "ugly": "\\\\supseteq", "pretty": "⊇" },
        { "ugly": "\\\\cup", "pretty": "∪" },
        { "ugly": "\\\\cap", "pretty": "∩" },
        { "ugly": "\\\\times", "pretty": "×" },
        { "ugly": "\\\\infty", "pretty": "∞" },
        { "ugly": "\\\\bot", "pretty": "⊥" },
        { "ugly": "\\\\top", "pretty": "⊤" },
        { "ugly": "\\\\land", "pretty": "∧" },
        { "ugly": "\\\\lor", "pretty": "∨" },
        { "ugly": "\\\\leftarrow", "pretty": "←" },
        { "ugly": "\\\\longleftarrow", "pretty": "⟵" },
        { "ugly": "\\\\Leftarrow", "pretty": "⇐" },
        { "ugly": "\\\\Longleftarrow", "pretty": "⟸" },
        { "ugly": "\\\\rightarrow", "pretty": "→" },
        { "ugly": "\\\\longrightarrow", "pretty": "⟶" },
        { "ugly": "\\\\Rightarrow", "pretty": "⇒" },
        { "ugly": "\\\\Longrightarrow", "pretty": "⟹" },
        { "ugly": "\\\\iff", "pretty": "⇔" },
        { "ugly": "\\\\leftrightarrow", "pretty": "↔" },
        { "ugly": "\\\\uparrow", "pretty": "↑" },
        { "ugly": "\\\\downarrow", "pretty": "↓" },
        { "ugly": "\\\\Uparrow", "pretty": "⇑" },
        { "ugly": "\\\\Downarrow", "pretty": "⇓" },
        { "ugly": "\\\\alpha", "pretty": "α" },
        { "ugly": "\\\\beta", "pretty": "β" },
        { "ugly": "\\\\gamma", "pretty": "γ" },
        { "ugly": "\\\\delta", "pretty": "δ" },
        { "ugly": "\\\\Delta", "pretty": "Δ" },
        { "ugly": "\\\\eta", "pretty": "η" },
        { "ugly": "\\\\varepsilon", "pretty": "ε" },
        { "ugly": "\\\\rho", "pretty": "ρ" },
        { "ugly": "\\\\sigma", "pretty": "σ" },
        { "ugly": "\\\\Sigma", "pretty": "Σ" },
        { "ugly": "\\\\sum", "pretty": "Σ" },
        { "ugly": "\\\\theta", "pretty": "θ" },
        { "ugly": "\\\\Theta", "pretty": "Θ" },
        { "ugly": "\\\\phi", "pretty": "φ" },
        { "ugly": "\\\\varphi", "pretty": "ϕ" },
        { "ugly": "\\\\Phi", "pretty": "Φ" },
        { "ugly": "\\\\lambda", "pretty": "λ" },
        { "ugly": "\\\\mu", "pretty": "μ" },
        { "ugly": "\\\\psi", "pretty": "ψ" },
        { "ugly": "\\\\pi", "pretty": "π" },
        { "ugly": "\\\\Pi", "pretty": "Π" },
        { "ugly": "\\\\omega", "pretty": "ω" },
        { "ugly": "\\\\Omega", "pretty": "Ω" },
        { "ugly": "\\\\mathbb\\{A\\}", "pretty": "𝔸" },
        { "ugly": "\\\\mathbb\\{B\\}", "pretty": "𝔹" },
        { "ugly": "\\\\mathbb\\{C\\}", "pretty": "ℂ" },
        { "ugly": "\\\\mathbb\\{D\\}", "pretty": "ⅅ" },
        { "ugly": "\\\\mathbb\\{E\\}", "pretty": "𝔼" },
        { "ugly": "\\\\mathbb\\{F\\}", "pretty": "𝔽" },
        { "ugly": "\\\\mathbb\\{G\\}", "pretty": "𝔾" },
        { "ugly": "\\\\mathbb\\{H\\}", "pretty": "ℍ" },
        { "ugly": "\\\\mathbb\\{I\\}", "pretty": "𝕀" },
        { "ugly": "\\\\mathbb\\{J\\}", "pretty": "𝕁" },
        { "ugly": "\\\\mathbb\\{K\\}", "pretty": "𝕂" },
        { "ugly": "\\\\mathbb\\{L\\}", "pretty": "𝕃" },
        { "ugly": "\\\\mathbb\\{M\\}", "pretty": "𝕄" },
        { "ugly": "\\\\mathbb\\{N\\}", "pretty": "ℕ" },
        { "ugly": "\\\\mathbb\\{O\\}", "pretty": "𝕆" },
        { "ugly": "\\\\mathbb\\{P\\}", "pretty": "ℙ" },
        { "ugly": "\\\\mathbb\\{Q\\}", "pretty": "ℚ" },
        { "ugly": "\\\\mathbb\\{R\\}", "pretty": "ℝ" },
        { "ugly": "\\\\mathbb\\{S\\}", "pretty": "𝕊" },
        { "ugly": "\\\\mathbb\\{T\\}", "pretty": "𝕋" },
        { "ugly": "\\\\mathbb\\{U\\}", "pretty": "𝕌" },
        { "ugly": "\\\\mathbb\\{V\\}", "pretty": "𝕍" },
        { "ugly": "\\\\mathbb\\{W\\}", "pretty": "𝕎" },
        { "ugly": "\\\\mathbb\\{X\\}", "pretty": "𝕏" },
        { "ugly": "\\\\mathbb\\{Y\\}", "pretty": "𝕐" },
        { "ugly": "\\\\mathbb\\{Z\\}", "pretty": "ℤ" },
        { "ugly": "\\\\chapter", "post": "{.+?}", "pretty": "§" },
        { "ugly": "\\\\section", "post": "{.+?}", "pretty": "§§" },
        { "ugly": "\\\\subsection", "post": "{.+?}", "pretty": "§§§" },
        { "ugly": "\\\\subsubsection", "post": "{.+?}", "pretty": "§§§§" },

        // https://github.com/Pancaek/vsc-conceal/issues/15
        { "ugly": "(?:\\\\hat){", "pretty": " ̑", "style": {"color": "#98C379"} },
        { "ugly": "(?<=\\\\hat{[^}]+)}", "pretty": "\b", "style": {"color": "#98C379"} },
        // { "ugly": "(?:\\\\ket{)", "pretty": "|", "style": {"color": "#E5C07B"} },
        { "ugly": "(?:\\\\ket{)", "pretty": "|", "style": {"color": "#98C379"} },
        { "ugly": "(?<=\\\\ket{[^}]+)}", "pretty": "⟩", "style": {"color": "#98C379"} },
        { "ugly": "(?:\\\\bra{)", "pretty": "⟨", "style": {"color": "#98C379"} },
        { "ugly": "(?<=\\\\bra{[^}]+)}", "pretty": "|", "style": {"color": "#98C379"} },

        { "ugly": "(?:\\_{?)", "pretty": "\b", "style": {"color": "red"} },
        { "ugly": "(?<=\\_{[^}]*)}", "pretty": "\b", "style": {"color": "red"} },
        { "ugly": "(?<=\\_{[^}]*)+|(?<=\\_)+", "pretty": "₊", "style": {"color": "#C678DD"} },
        { "ugly": "(?<=\\_{[^}]*)-|(?<=\\_)-", "pretty": "₋", "style": {"color": "#C678DD"} },
        { "ugly": "(?<=\\_{[^}]*)=|(?<=\\_)=", "pretty": "₌", "style": {"color": "#C678DD"} },
        { "ugly": "(?<=\\_{[^}]*)\\(|(?<=\\_)\\(", "pretty": "₍", "style": {"color": "#C678DD"} },
        { "ugly": "(?<=\\_{[^}]*)\\)|(?<=\\_)\\)", "pretty": "₎", "style": {"color": "#C678DD"} },
        { "ugly": "(?<=\\_{[^}]*)0|(?<=\\_)0", "pretty": "₀", "style": {"color": "#C678DD"} },
        { "ugly": "(?<=\\_{[^}]*)1|(?<=\\_)1", "pretty": "₁", "style": {"color": "#C678DD"} },
        { "ugly": "(?<=\\_{[^}]*)2|(?<=\\_)2", "pretty": "₂", "style": {"color": "#C678DD"} },
        { "ugly": "(?<=\\_{[^}]*)3|(?<=\\_)3", "pretty": "₃", "style": {"color": "#C678DD"} },
        { "ugly": "(?<=\\_{[^}]*)4|(?<=\\_)4", "pretty": "₄", "style": {"color": "#C678DD"} },
        { "ugly": "(?<=\\_{[^}]*)5|(?<=\\_)5", "pretty": "₅", "style": {"color": "#C678DD"} },
        { "ugly": "(?<=\\_{[^}]*)6|(?<=\\_)6", "pretty": "₆", "style": {"color": "#C678DD"} },
        { "ugly": "(?<=\\_{[^}]*)7|(?<=\\_)7", "pretty": "₇", "style": {"color": "#C678DD"} },
        { "ugly": "(?<=\\_{[^}]*)8|(?<=\\_)8", "pretty": "₈", "style": {"color": "#C678DD"} },
        { "ugly": "(?<=\\_{[^}]*)9|(?<=\\_)9", "pretty": "₉", "style": {"color": "#C678DD"} },
        // { "ugly": "(?<=\\_{[^}\\]*)?|(?<=\\_)?", "pretty": "ₔ", "style": {"color": "#C678DD"} },
        { "ugly": "(?<=\\_{[^}]*)a|(?<=\\_)a", "pretty": "ₐ", "style": {"color": "#C678DD"} },
        { "ugly": "(?<=\\_{[^}]*)e|(?<=\\_)e", "pretty": "ₑ", "style": {"color": "#C678DD"} },
        { "ugly": "(?<=\\_{[^}]*)o|(?<=\\_)o", "pretty": "ₒ", "style": {"color": "#C678DD"} },
        { "ugly": "(?<=\\_{[^}]*)x|(?<=\\_)x", "pretty": "ₓ", "style": {"color": "#C678DD"} },
        { "ugly": "(?<=\\_{[^}]*)h|(?<=\\_)h", "pretty": "ₕ", "style": {"color": "#C678DD"} },
        { "ugly": "(?<=\\_{[^}]*)k|(?<=\\_)k", "pretty": "ₖ", "style": {"color": "#C678DD"} },
        { "ugly": "(?<=\\_{[^}]*)l|(?<=\\_)l", "pretty": "ₗ", "style": {"color": "#C678DD"} },
        { "ugly": "(?<=\\_{[^}]*)m|(?<=\\_)m", "pretty": "ₘ", "style": {"color": "#C678DD"} },
        { "ugly": "(?<=\\_{[^}]*)n|(?<=\\_)n", "pretty": "ₙ", "style": {"color": "#C678DD"} },
        { "ugly": "(?<=\\_{[^}]*)p|(?<=\\_)p", "pretty": "ₚ", "style": {"color": "#C678DD"} },
        { "ugly": "(?<=\\_{[^}]*)s|(?<=\\_)s", "pretty": "ₛ", "style": {"color": "#C678DD"} },
        { "ugly": "(?<=\\_{[^}]*)t|(?<=\\_)t", "pretty": "ₜ", "style": {"color": "#C678DD"} },

        { "ugly": "(?:\\^{?)", "pretty": "\b", "style": {"color": "red"} },
        { "ugly": "(?<=\\^{[^}]*)}", "pretty": "\b", "style": {"color": "red"} },
        { "ugly": "(?<=\\^{[^}]*)+|(?<=\\^)+", "pretty": "⁺", "style": {"color": "#C678DD"} },
        { "ugly": "(?<=\\^{[^}]*)-|(?<=\\^)-", "pretty": "⁻", "style": {"color": "#C678DD"} },
        { "ugly": "(?<=\\^{[^}]*)=|(?<=\\^)=", "pretty": "⁼", "style": {"color": "#C678DD"} },
        { "ugly": "(?<=\\^{[^}]*)\\(|(?<=\\^)\\(", "pretty": "⁽", "style": {"color": "#C678DD"} },
        { "ugly": "(?<=\\^{[^}]*)\\)|(?<=\\^)\\)", "pretty": "⁾", "style": {"color": "#C678DD"} },
        { "ugly": "(?<=\\^{[^}]*)\\\\?<|(?<=\\^)\\\\?<", "pretty": "˂", "style": {"color": "#C678DD"} },
        { "ugly": "(?<=\\^{[^}]*)\\\\?>|(?<=\\^)\\\\?>", "pretty": "˃", "style": {"color": "#C678DD"} },
        { "ugly": "(?<=\\^{[^}]*)0|(?<=\\^)0", "pretty": "⁰", "style": {"color": "#C678DD"} },
        { "ugly": "(?<=\\^{[^}]*)1|(?<=\\^)1", "pretty": "¹", "style": {"color": "#C678DD"} },
        { "ugly": "(?<=\\^{[^}]*)2|(?<=\\^)2", "pretty": "²", "style": {"color": "#C678DD"} },
        { "ugly": "(?<=\\^{[^}]*)3|(?<=\\^)3", "pretty": "³", "style": {"color": "#C678DD"} },
        { "ugly": "(?<=\\^{[^}]*)4|(?<=\\^)4", "pretty": "⁴", "style": {"color": "#C678DD"} },
        { "ugly": "(?<=\\^{[^}]*)5|(?<=\\^)5", "pretty": "⁵", "style": {"color": "#C678DD"} },
        { "ugly": "(?<=\\^{[^}]*)6|(?<=\\^)6", "pretty": "⁶", "style": {"color": "#C678DD"} },
        { "ugly": "(?<=\\^{[^}]*)7|(?<=\\^)7", "pretty": "⁷", "style": {"color": "#C678DD"} },
        { "ugly": "(?<=\\^{[^}]*)8|(?<=\\^)8", "pretty": "⁸", "style": {"color": "#C678DD"} },
        { "ugly": "(?<=\\^{[^}]*)9|(?<=\\^)9", "pretty": "⁹", "style": {"color": "#C678DD"} },
        // { "ugly": "(?<=\\^{[^}\\]*)?|(?<=\\^)?", "pretty": "ₔ", "style": {"color": "#C678DD"} },
        { "ugly": "(?<=\\^{[^}]*)a|(?<=\\^)a", "pretty": "ᵃ", "style": {"color": "#C678DD"} },
        { "ugly": "(?<=\\^{[^}]*)b|(?<=\\^)b", "pretty": "ᵇ", "style": {"color": "#C678DD"} },
        { "ugly": "(?<=\\^{[^}]*)d|(?<=\\^)d", "pretty": "ᵈ", "style": {"color": "#C678DD"} },
        { "ugly": "(?<=\\^{[^}]*)e|(?<=\\^)e", "pretty": "ᵉ", "style": {"color": "#C678DD"} },
        { "ugly": "(?<=\\^{[^}]*)g|(?<=\\^)g", "pretty": "ᵍ", "style": {"color": "#C678DD"} },
        { "ugly": "(?<=\\^{[^}]*)h|(?<=\\^)h", "pretty": "ʰ", "style": {"color": "#C678DD"} },
        { "ugly": "(?<=\\^{[^}]*)i|(?<=\\^)i", "pretty": "ⁱ", "style": {"color": "#C678DD"} },
        { "ugly": "(?<=\\^{[^}]*)n|(?<=\\^)n", "pretty": "ⁿ", "style": {"color": "#C678DD"} },
        { "ugly": "(?<=\\^{[^}]*)m|(?<=\\^)m", "pretty": "ᵐ", "style": {"color": "#C678DD"} },
        { "ugly": "(?<=\\^{[^}]*)p|(?<=\\^)p", "pretty": "ᵖ", "style": {"color": "#C678DD"} },
        { "ugly": "(?<=\\^{[^}]*)s|(?<=\\^)s", "pretty": "ˢ", "style": {"color": "#C678DD"} },
        { "ugly": "(?<=\\^{[^}]*)t|(?<=\\^)t", "pretty": "ᵗ", "style": {"color": "#C678DD"} },
        { "ugly": "\\s*\\\\times\\s+", "pretty": "·" },
        { "ugly": "\\\\sqrt", "pretty": "√" },
        { "ugly": "\\s*\\\\oplus\\s+", "pretty": " ⊕ " },
        { "ugly": "\\s*\\\\otimes\\s+", "pretty": " ⊗ " },
        { "ugly": "\\\\\\\\\\\\\\\\", "pretty": "↵↵" },
        { "ugly": "\\\\\\\\", "pretty": "↵" },
        { "ugly": "\\\\left\\\\vert", "pretty": "|" },
        { "ugly": "\\\\right\\\\vert", "pretty": "|" },
      ]
    }
  ],
  "editor.formatOnSave": false
  // "latex-workshop.view.autoFocus.enabled": true
}
